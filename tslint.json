{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-no-unused", "tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "curly": true,
    "import-name": false,
    "prefer-array-literal": false
  }
}
