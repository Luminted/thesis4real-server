{
  "name": "server",
  "version": "1.0.0",
  "main": "build/index.js",
  "scripts": {
    "start": "node -r dotenv/config ./build/index.js",
    "start:dev": "node -r ts-node/register -r dotenv/config ./src/index.ts",
    "start:debug": "NODE_ENV=development npm run start -- --inspect=9229 ",
    "dev": "NODE_ENV=development nodemon",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "test": "mocha './src/**/*.test.ts' -r ts-node/register",
    "test:debug": "mocha ./src/handlers/Table/tests/handleJoinTable.test.ts --inspect=5859 -r ts-node/register",
    "clean": "rimraf ./build",
    "lint": "tslint --fix -c tslint.json -p ./tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pacote/shuffle": "^1.0.6",
    "hashmap": "^2.4.0",
    "immer": "^6.0.5",
    "immutable": "^4.0.0-rc.12",
    "install": "^0.13.0",
    "lodash.throttle": "^4.1.1",
    "lodash.clonedeep": "^4.5.0",
    "short-uuid": "^3.1.1",
    "socket.io": "^2.3.0",
    "typescript-immutable": "^1.0.0",
    "typescript-ioc": "^3.2.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
  },
  "devDependencies": {
    "@typed/hashmap": "^1.0.1",
    "@types/express": "^4.17.6",
    "@types/hashmap": "^2.3.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/sinon": "^9.0.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "dotenv": "^8.2.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-unused": "^0.2.0-alpha.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.8.2"
  }
}
